<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TM" parent="Theme.Material3.Light">
        <item name="windowNoTitle">true</item>

        <item name="colorPrimary">#f25609</item>

        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.TMDark" parent="Theme.Material3.Dark">
        <item name="windowNoTitle">true</item>

        <item name="colorPrimary">#f25609</item>

        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.TM.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TM.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TM.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>